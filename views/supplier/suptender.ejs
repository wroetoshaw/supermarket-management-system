        <div class="col-md-4">
            <h3>PRODUCT MRP (In Rs)</h3>
        </div>
        <div class="col-md-4">
          <h3>YOUR QUOTE (In Rs)</h3>
      </div>
    </div>
     <% for(x in responses){%>
          <div class="row">
              <div class="col-md-4">
                  <h3><%=responses[x].p_name%></h3>
              </div>
              <div class="col-md-4">
                  <h3><%=responses[x].p_mrp%></h3>
              </div>
              <div class="col-md-4">
                <h3><%=responses[x].cost%></h3>
            </div>
          </div>
      <%}%>


