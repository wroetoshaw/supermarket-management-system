      <div class="navbar-nav">
        <a class="nav-link" href="/logout">Logout</a>
      </div>
    </div>
  </nav>

  <div class="container my-5">
      <h1>responses Details</h1>
      <div class="row">
        <div class="col-md-6">
            <h3>responses Name: </h3>
        </div>
          <div class="col-md-6">
              <h3><%=responses[0].t_name%></h3>
          </div>
      </div>
      <div class="row">
        <div class="col-md-6">
            <h3>responses Name: </h3>
        </div>
          <div class="col-md-6">
              <h3><%=responses[0].t_name%></h3>
          </div>
      </div>
      <div class="row">
        <div class="col-md-6">
            <h3>Start Time: </h3>
        </div>
          <div class="col-md-6">
              <h3><%=responses[0].t_opentime.toISOString()%></h3>
          </div>
      </div>
      <div class="row">
        <div class="col-md-6">
